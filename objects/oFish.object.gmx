    rx = sign(rx)*8;
    }
if abs(ry) &lt; 8 {
    ry = sign(ry)*8;
    }
    
//Target coordinate
if !(vCapture) {
    xx = ox + (rx/4);
    yy = oy + (ry/4);
    }
    else
    {
    xx = oPlayer.x + rx;
    yy = oPlayer.y + ry;
    }

x -= (x - xx)/48;
y -= (y - yy)/48;

//Enable capture
if !(vCapture) &amp;&amp; ( point_distance( x, y, oPlayer.x, oPlayer.y ) &lt; 64 ) &amp;&amp; ( keyboard_check(ord( 'X' )) || (mouse_check_button( mb_right ))) {
