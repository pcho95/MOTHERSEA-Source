    timer1 = 0;
    rx = round( random( 32 + (global.fishCount/2) ) - random( 32 + (global.fishCount/2) ) );
    ry = round( random( 32 + (global.fishCount/2) ) - random( 32 + (global.fishCount/2) ) );
    }
    
